{
  "scripts": {
    "develop": "nodemon node dist/index.js",
    "deploy": "npm run db:migrate && npm run build && node dist/index.js",
    "build": "tsc",
    "watch": "tsc -w",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:generate": "sequelize-cli migration:generate --name ",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:generate": "sequelize-cli seed:generate --name ",
    "db:drop": "sequelize-cli db:drop",
    "db:create": "sequelize-cli db:create",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate",
    "db:setup": "npm run db:reset && npm run db:seed"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^13.13.5",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/validator": "^13.0.0",
    "eslint": "^9.7.0",
    "globals": "^15.8.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "sequelize-cli": "^5.5.1",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "sequelize": "^6.37.3"
  }
}
